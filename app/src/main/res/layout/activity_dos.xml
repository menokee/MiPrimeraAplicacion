<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DosActivity">

    <TextView
            android:text="@string/mensaje"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mrrmsj" android:textSize="30sp"
            android:textColor="@color/design_default_color_primary" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="332dp"
            android:layout_marginStart="148dp"/>

    <Button
            android:text="@string/llamada"
            android:layout_width="200dp"
            android:layout_height="0dp"
            android:id="@+id/llamar" tools:ignore="MissingConstraints"
            android:layout_marginTop="161dp" android:layout_marginBottom="203dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/camara"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="@string/camara"
            android:layout_width="194dp"
            android:layout_height="0dp"
            android:id="@+id/camara" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/llamar" android:layout_marginBottom="223dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>